global:
  checkNewVersion: true
  sendAnonymousUsage: false

log:
  level: ERROR

api:
 dashboard: true  # true by default
 insecure: true  # Don't do this in production!

entryPoints:
  web:
    address: :80
  websecure:
    address: :443

providers:
  docker:
    network: homelab
    exposedByDefault: false
  file:
    directory: /etc/traefik
    watch: true
